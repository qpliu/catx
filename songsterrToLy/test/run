#!/bin/sh
set -e
./build
java -jar 1.jar --name bassPart - <test/bass.html >test/bass.ly
java -jar 1.jar --name bassPart --shift 21/4 - <test/bass.html >test/bass_shift.ly
java -jar 1.jar --name drsPart - <test/drs.html >test/drs.ly
java -jar 1.jar --name voLyrics --lyrics - <test/lyrics.html >test/lyrics.ly
java -jar 1.jar --name synthPart - <test/synth.html >test/synth.ly
