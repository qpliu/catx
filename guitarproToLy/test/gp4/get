#!/bin/sh
set -e
rm -f generated-*
java -jar ../../1.jar <application_guitar_pro4-25940548.gp4 \
verbose 2 \
scale "" \
track 0 \
    preset kav \
    generate-lyrics \
    output-lyrics \
    output-lilypond \
track 0 \
    preset peter \
    output-chords \
    output-dotext \
    output-lilypond \
track 1 \
    preset peter \
    output-lilypond \
track 2 \
    preset peter \
    output-dotext \
    output-lilypond \
track 3 \
    preset peter \
    output-dotext \
    output-lilypond \
track 4 \
    preset midiOne \
    output-lilypond \
track 5 \
    preset bass \
    output-lilypond \
track 6 \
    preset gg \
    output-drums \
2>&1|tee log
