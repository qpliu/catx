#!/bin/sh
set -e
rm -f generated-*
java -jar ../../1.jar <"/share/mirror/songsterr/stuff/d-keep/d-The Beatles/d-Don't Pass Me By-s61425/d-r77210/application_guitar_pro3-3677427.gp3" \
scale "" \
track 0 \
    name volyrics \
    layout-who "allPart kavPart" \
    instrument-name Vocals \
    instrument-short-name Vo \
    output-lyrics \
track 0 \
    name karaoke \
    midi-who midiKaraoke \
    output-karaoke \
track 0 \
    name guitar \
    layout-who "allPart peterPart" \
    instrument-name Guitar \
    instrument-short-name Gtr \
    midi-who midiPeter \
    layout-extra "\\clef \"treble_8\"" \
    layout-tabs \
    string-numbers \
track 2 \
    name violin \
    midi-who midiOne \
track 3 \
    name rpiano \
    midi-who midiOne \
track 4 \
    name organ \
    midi-who midiOne \
track 5 \
    name lpiano \
    midi-who midiOne \
track 6 \
    name bass \
    layout-who "allPart bassPart" \
    layout-extra "\\clef \"bass_8\"" \
    instrument-name Bass \
    instrument-short-name Ba \
    midi-who midiBass \
    layout-tabs \
    string-numbers \
track 7 \
    name drs \
    layout-who "allPart ggPart" \
    instrument-name Drums \
    instrument-short-name Dr \
    midi-who midiGg \
2>&1|tee log
