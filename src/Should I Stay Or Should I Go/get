#!/bin/sh
# https://www.songsterr.com/api/meta/22070/revisions
# https://gp.songsterr.com/export.266806564.gp
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key d \major' \
track 4 preset kav output-lilypond output-lyrics which-lyrics text \
track 0 preset peter output-lilypond \
track 1 preset peter output-lilypond \
track 2 preset gg output-drums \
track 3 preset bass output-lilypond \
track 4 preset susan output-lilypond output-lyrics which-lyrics text
