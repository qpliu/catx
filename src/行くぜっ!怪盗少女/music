midimusic = <<
    \markupStuff
    \tag #'(midiSusan) \new Staff = "vo1text" \midivoOneText
    \tag #'(midiSusan) \new Staff = "vo1" \voOnePart
    \tag #'(midiSusan) \new Staff = "vo2text" \midivoTwoText
    \tag #'(midiSusan) \new Staff = "vo2" \voTwoPart
    \tag #'(midiOne) \new Staff = "key1text" \midikeyOneText
    \tag #'(midiOne) \new Staff = "key1" \keyOnePart
    \tag #'(midiOne) \new Staff = "key2text" \midikeyTwoText
    \tag #'(midiOne) \new Staff = "key2" \keyTwoPart
    \tag #'(midiOne) \new Staff = "key3text" \midikeyThreeText
    \tag #'(midiOne) \new Staff = "key3" \keyThreePart
    \tag #'(midiOne) \new Staff = "key4text" \midikeyFourText
    \tag #'(midiOne) \new Staff = "key4" \keyFourPart
    \tag #'(midiOne) \new Staff = "key5text" \midikeyFiveText
    \tag #'(midiOne) \new Staff = "key5" \keyFivePart
    \tag #'(midiPeter) \new Staff = "eg1text" \midiegOneText
    \tag #'(midiPeter) \new Staff = "eg1" \egOnePart
    \tag #'(midiPeter) \new Staff = "eg2text" \midiegTwoText
    \tag #'(midiPeter) \new Staff = "eg2" \egTwoPart
    \tag #'(midiPeter) \new Staff = "eg4text" \midiegFourText
    \tag #'(midiPeter) \new Staff = "eg4" \egFourPart
    \tag #'(midiPeter) \new Staff = "eg5text" \midiegFiveText
    \tag #'(midiPeter) \new Staff = "eg5" \egFivePart
    \tag #'(midiSusan) \new Staff \with {
	midiInstrument = #"choir aahs"
	midiMinimumVolume = #0
	midiMaximumVolume = #.5
    } \choOnePart
    \tag #'(midiTwo) \new Staff \with {
	midiInstrument = #"electric bass (finger)"
	midiMinimumVolume = #0
	midiMaximumVolume = #.7
    } \ebPart
    \tag #'(midiTwo) \new Staff \with {
	midiInstrument = #"timpani"
	midiMinimumVolume = #0
	midiMaximumVolume = #.8
    } \percTwoPart
    \tag #'(midiGg) \new Staff = "drstext" \mididrsText
    \tag #'(midiGg) \new DrumStaff = "drs" \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
    \tag #'(midiTwo) \new Staff = "perc1text" \midipercOneText
    \tag #'(midiTwo) \new DrumStaff = "perc1" \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \percOnePart
    \tag #'(midiTwo) \new Staff = "setext" \midiseText
    \tag #'(midiTwo) \new DrumStaff = "se" \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \sePart
>>
