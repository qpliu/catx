	\lyricsegOneText
    >>
    \tag #'(systemOnePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "E.G.2" }
	shortInstrumentName = \markup { \rotate #90 "E.G.2" }
    } <<
	\markupStuff
	\clef "treble_8"
