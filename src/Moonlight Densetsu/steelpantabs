    3-5.4t4 2-4.3t r2 |
    r1 |
    r2 r8 1-4t 3t 1t |
    1-3t4 1t 2-4t8 3t4 1t8~ |
    2-1t4 s2. |
\mymark "A" #12
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "B" #20
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "C" #28
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "D" #36
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "E" #44
