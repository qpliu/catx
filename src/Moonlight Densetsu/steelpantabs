    4-3t4 2-3t 4-1t 2-1t |
    4-4t8 3-0t r2 2-4t8 3t |
    3-3t8 4t 1-4t 3t s2 |
\mymark "B-2" #48
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "C-2" #56
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "D-2" #64
    s1 s1 s1 s1 s1 s1 s1 s1 |
\mymark "F" #72
