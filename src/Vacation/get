#!/bin/sh
# https://www.songsterr.com/api/meta/55978/revisions
# https://d12drcwhcokzqv.cloudfront.net/3672549.gp5
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key f \major' \
track 0 preset kav output-lilypond output-lyrics which-lyrics text scale bes \
track 2 preset peter output-lilypond \
track 3 preset bass output-lilypond \
track 1 preset gg output-drums \
track 4 preset susan output-lilypond no-chords false scale bes \
track 5 preset midiOne output-lilypond
