    \tag #'(allPart ggPart kavPart) \new DrumStaff \with {
        instrumentName = \markup { \rotate #90 "Drums" }
        shortInstrumentName = \markup { \rotate #90 "Drs" }
        drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
        \markupStuff
        \drs
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiKaraoke) \new Staff = "generated-vocals" \with {
        midiInstrument = #"tenor sax"
    } \vocals
    \tag #'(midiKaraoke) \vocalsKaraoke
    \tag #'(midiSusan) \new Staff = "generated-steelpan" \with {
        midiInstrument = #"tenor sax"
    } \steelpan
    \tag #'(midiPeter) \new Staff = "generated-guitar" \with {
        midiInstrument = #"electric guitar (clean)"
    } \guitar
    \tag #'(midiPeter) \new Staff = "generated-guitarTwo" \with {
        midiInstrument = #"electric guitar (clean)"
    } \guitarTwo
    \tag #'(midiPeter) \new Staff = "generated-guitarThree" \with {
        midiInstrument = #"overdriven guitar"
    } \guitarThree
    \tag #'(midiPeter) \new Staff = "generated-guitarFour_dotext" \guitarFourDotext
    \tag #'(midiPeter) \new Staff = "generated-guitarFour" \with {
        midiInstrument = #"distorted guitar"
    } \guitarFour
    \tag #'(midiPeter) \new Staff = "generated-guitarFive_dotext" \guitarFiveDotext
    \tag #'(midiPeter) \new Staff = "generated-guitarFive" \with {
        midiInstrument = #"distorted guitar"
    } \guitarFive
    \tag #'(midiPeter) \new Staff = "generated-guitarSix_dotext" \guitarSixDotext
    \tag #'(midiPeter) \new Staff = "generated-guitarSix" \with {
        midiInstrument = #"distorted guitar"
    } \guitarSix
    \tag #'(midiPeter) \new Staff = "generated-guitarSeven" \with {
        midiInstrument = #"electric guitar (clean)"
    } \guitarSeven
    \tag #'(midiBass) \new Staff = "generated-bass" \with {
        midiInstrument = #"slap bass 1"
    } \bass
    \tag #'(midiGg) \new DrumStaff = "generated-drs" \with {
        drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drs
>>
