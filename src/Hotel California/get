#!/bin/sh
WHERE="/share/mirror/songsterr/stuff/d-keep/d-The Eagles/d-Hotel California-s447/d-r414164"

gzip -d <"$WHERE/z-t0"|java -jar ../../songsterrToLy/1.jar --name voPart --output-tabs - >generated-votabs
gzip -d <"$WHERE/z-t0"|java -jar ../../songsterrToLy/1.jar --name voLyrics --lyrics - >generated-lyrics
gzip -d <"$WHERE/z-t1"|java -jar ../../songsterrToLy/1.jar --name guitarOnePart --output-tabs - >generated-guitar1tabs
gzip -d <"$WHERE/z-t2"|java -jar ../../songsterrToLy/1.jar --name guitarTwoPart --output-tabs - >generated-guitar2tabs
gzip -d <"$WHERE/z-t3"|java -jar ../../songsterrToLy/1.jar --name guitarThreePart --output-tabs - >generated-guitar3tabs
gzip -d <"$WHERE/z-t4"|java -jar ../../songsterrToLy/1.jar --name guitarFourPart --output-tabs - >generated-guitar4tabs
gzip -d <"$WHERE/z-t5"|java -jar ../../songsterrToLy/1.jar --name guitarFivePart --output-tabs - >generated-guitar5tabs
gzip -d <"$WHERE/z-t6"|java -jar ../../songsterrToLy/1.jar --name guitarSixPart --output-tabs - >generated-guitar6tabs
gzip -d <"$WHERE/z-t7"|java -jar ../../songsterrToLy/1.jar --name bassPart --output-tabs - >generated-basstabs
#gzip -d <"$WHERE/z-t8"|java -jar ../../songsterrToLy/1.jar --name drumsPart - >generated-drumscolors
gzip -d <"$WHERE/z-t9"|java -jar ../../songsterrToLy/1.jar --name guitarSevenPart --output-tabs - >generated-guitar7tabs
