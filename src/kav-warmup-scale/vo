voPart = {
  \time 4/4
  \tempo 4 = 60
  \key c \major \relative { c'4 d e f | g a b c | c b a g | f e d c |  r1 }
  \key cis \major \relative { cis'4 dis eis  fis | gis ais bis cis | cis bis ais gis | fis eis dis cis | r1 }
  \key d \major \relative { d'4 e fis g | a b cis d | d cis b a | g fis e d | r1 }
  \key ees \major \relative { ees'4 f g aes | bes c d ees | ees d c bes | aes g f ees | r1 }
  \key e \major \relative { e'4 fis gis a | b cis dis e | e dis cis b | a gis fis e | r1 }
  \key f \major \relative { f'4 g a bes | c d e f | f e d c | bes a g f | r1 }
  \key fis \major \relative { fis'4 gis ais b | cis dis eis  fis | fis eis dis cis b | ais gis fis | r1 }
  \key g \major \relative { g'4 a b c | d e fis g | g fis e d | c b a g | r1 }
  \key aes \major \relative { aes'4 bes c des | ees f g aes | aes g f ees | des c bes aes | r1 }
  \key a \major \relative { a'4 b cis d | e fis gis a | a gis fis e | d cis b a | r1 }
  \key bes \major \relative { bes'4 c d ees | f g a bes | bes a g f | ees d c bes | r1 }
  \key b \major \relative { b'4  cis dis e | fis gis ais b | b ais gis fis | e dis cis b | r1 }
}
