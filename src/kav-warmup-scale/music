% BIN midi all

\header {
    title = "Warmup Scale"
}

\include "markup"
\include "vo"
\include "lyrics"
\include "karaoke"

layoutmusic = <<
    \tag #'(allPart) \new Staff <<
	\markupStuff
	\voPart
	\voLyrics
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiKaraoke) \new Staff \with {
	midiInstrument = #"flute" % 73
    } <<
	\karaokeLyrics
	\transpose c c, \voPart
    >>
>>
