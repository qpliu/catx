    << { <cymc cymcb>8 r <cymc sn>4 cymc8 r <cymc sn> r | } \context DrumVoice = "drsb" { bda8 bda r4 bda8 bda r bda | } >>
    << { cymc8 r <cymc sn> r cymc r <cymc sn>4 | } \context DrumVoice = "drsb" { r8 bda r bda r bda r4 | } >>
    << { <cymc cymcb>8 r <cymc sn>4 cymc8 r <cymc sn>4 | } \context DrumVoice = "drsb" { bda8 bda r4 bda8 bda r4 | } >>
    << { <cymc cymcb>8 r <cymc sn>4 cymc8 r <cymc sn>4 | } \context DrumVoice = "drsb" { bda8 bda r4 bda8 bda r4 | } >>
