#!/bin/sh
# https://www.songsterr.com/api/meta/402432/revisions
# https://d12drcwhcokzqv.cloudfront.net/17643593.gp5
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key a \major' \
track 0 preset kav output-lilypond output-lyrics which-lyrics text \
track 1 preset peter output-lilypond \
track 3 preset bass output-lilypond \
track 4 preset gg output-drums \
track 5 preset susan output-lilypond
