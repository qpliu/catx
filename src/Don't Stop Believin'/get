#!/bin/sh
WHERE="/share/mirror/songsterr/stuff/d-keep/d-Journey/d-Don't Stop Believin'-s10681/d-r252847/application_guitar_pro5-16160530.gp5"
set -e
rm -f generated-*
java -jar ../../guitarproToLy/1.jar <"$WHERE" \
markup-extra '\key e \major' \
modified-filename drs \
modified-filename vocals \
modified-filename vocals_karaoke \
modified-filename vocals_lyrics \
track 3 \
    preset kav \
    output-lilypond \
    output-lyrics \
    output-karaoke \
    output-dotext \
track 1 \
    preset susan \
track 2 \
    preset midiOne \
track 5 \
    preset midiTwo \
    output-dotext \
track 4 \
    preset peter \
    output-dotext \
track 0 \
    preset bass \
    output-dotext \
track 6 \
    preset gg \
2>&1|tee log
