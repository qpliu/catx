#!/bin/sh
# https://www.songsterr.com/api/meta/5198/revisions
# https://d12drcwhcokzqv.cloudfront.net/49643055.gp5
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key e \major' \
track 2 preset kav output-lilypond output-lyrics which-lyrics text \
track 0 preset peter output-lilypond \
track 4 preset bass output-lilypond \
track 5 preset gg output-drums \
track 2 preset susan output-lilypond output-lyrics which-lyrics text
