% PARTS midi
% BIN midi all

\header {
    title = "The One I Love"
    composer = "R.E.M."
}

#(set-global-staff-size 20)

\include "markup"
\include "drs"

layoutmusic = <<
    \tag #'(allPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drsPart
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
>>
