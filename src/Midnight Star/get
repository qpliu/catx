#!/bin/sh
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key e \minor' \
track 0 preset peter output-lilypond \
track 1 preset bass output-lilypond layout-who "allPart bassPart kavPart" \
track 2 preset susan output-lilypond \
track 4 preset kav output-lilypond output-lyrics which-lyrics 0 \
track 3 preset gg output-drums
