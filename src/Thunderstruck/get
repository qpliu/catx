#!/bin/sh
java -jar ../../songsterrToLy/1.jar --name guitarOne --output-tabs https://www.songsterr.com/a/wsa/ac-dc-thunderstruck-tab-s1352t2 > guitar1tabs
java -jar ../../songsterrToLy/1.jar --name guitarTwo --output-tabs https://www.songsterr.com/a/wsa/ac-dc-thunderstruck-tab-s1352t3 > guitar2tabs
java -jar ../../songsterrToLy/1.jar --name guitarThree --output-tabs https://www.songsterr.com/a/wsa/ac-dc-thunderstruck-tab-s1352t4 > guitar3tabs
java -jar ../../songsterrToLy/1.jar --name bass --output-tabs https://www.songsterr.com/a/wsa/ac-dc-thunderstruck-bass-tab-s1352t5 > basstabs
#java -jar ../../songsterrToLy/1.jar --name drums https://www.songsterr.com/a/wsa/ac-dc-thunderstruck-drum-tab-s1352t6 > drums
java -jar ../../songsterrToLy/1.jar --name drumsTwo https://www.songsterr.com/a/wsa/ac-dc-thunderstruck-drum-tab-s1352t7 > drums2
