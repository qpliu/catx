#!/bin/sh
SONG=https://www.songsterr.com/a/wsa/the-police-message-in-a-bottle-tab-s3706t1
DRUMMAP="44 hh 0,57 cymc 0,48 tomh 0,43 toml 0,41 toml 0,"
java <application_guitar_pro5-41910045.gp5 -jar ../../guitarproToLy/1.jar \
markup-extra '\key cis \minor' \
drumMap "$DRUMMAP" \
track 0 preset kav output-lilypond output-lyrics output-karaoke \
track 1 preset peter output-lilypond output-dotext \
track 3 preset bass output-lilypond \
track 4 preset gg output-drums

sed -e 's/gis8\\4/e8\\5/' < generated-guitar > tmp
mv tmp generated-guitar
