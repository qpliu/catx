% BIN midi all kav susan gg peter bass

\header {
    title = "Let It Go"
    composer = "Frozen"
}

\include "markup"
\include "generated-bass"
\include "generated-drums"
\include "generated-eguitar"
\include "generated-lguitar"
\include "generated-rguitar"
\include "generated-lpiano"
\include "generated-rpiano"
\include "vo"
\include "karaoke"
\include "lyrics"

layoutmusic = <<
    \tag #'(allPart kavPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Vocals" }
	shortInstrumentName = \markup { \rotate #90 "Vo" }
    } <<
	\markupStuff
	\clef treble
	\voPart
	\voLyrics
    >>
    \tag #'(allPart susanPart) \new PianoStaff \with {
	instrumentName = \markup { \rotate #90 "Steelpan" }
	shortInstrumentName = \markup { \rotate #90 "Sp" }
    } <<
	\new Staff = "upper" <<
	    \markupStuff
	    \clef treble \rpianoPart
	>>
	\new Staff = "lower" <<
	    \clef bass \lpianoPart
	    \voLyrics
	>>
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Lead Guitar" }
	    shortInstrumentName = \markup { \rotate #90 "L.G." }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \lguitarPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\lguitarPart
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Rhythm Guitar" }
	    shortInstrumentName = \markup { \rotate #90 "R.G." }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \rguitarPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\rguitarPart
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Extra Guitar" }
	    shortInstrumentName = \markup { \rotate #90 "E.G." }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \eguitarPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\eguitarPart
    >>
    \tag #'(allPart bassPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Bass" }
	shortInstrumentName = \markup { \rotate #90 "Ba" }
    } <<
	\markupStuff
	\clef "bass_8"
	\bassPart
    >>
    \tag #'(bassPart) \new TabStaff \with { stringTunings = #bass-tuning } <<
	\markupStuff
	\bassPart
    >>
    \tag #'(allPart kavPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Drums" }
	shortInstrumentName = \markup { \rotate #90 "Dr" }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
    >>
    \tag #'(ggPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
	\voLyrics
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drumsPart
    \tag #'(midiSusan) \new Staff \with {
	midiInstrument = #"steel drums"
    } <<
	\rpianoPart
	\lpianoPart
    >>
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"distorted guitar" % 30
	midiMinimumVolume = #0
	midiMaximumVolume = #.3
    } \lguitarPart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"distorted guitar" % 30
	midiMinimumVolume = #0
	midiMaximumVolume = #.3
    } \rguitarPart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"distorted guitar" % 30
	midiMinimumVolume = #0
	midiMaximumVolume = #.3
    } \eguitarPart
    \tag #'(midiBass) \new Staff \with {
	midiInstrument = #"electric bass (finger)" % 33
    } \bassPart
    \tag #'(midiKaraoke) \new Staff <<
	\karaokeLyrics
	\voPart
    >>
>>
