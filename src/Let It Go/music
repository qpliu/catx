% BIN midi all kav susan gg peter bass

\header {
    title = "Let It Go"
    composer = "Frozen"
}

\include "markup"
\include "generated-bass"
\include "drums"
\include "generated-guitar"
\include "piano1"
\include "generated-piano2"
\include "generated-piano3"
\include "generated-strings1"
\include "generated-strings2"
\include "generated-vo"
\include "karaoke"
\include "lyrics"

layoutmusic = <<
    \tag #'(allPart kavPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Vocals" }
	shortInstrumentName = \markup { \rotate #90 "Vo" }
    } <<
	\markupStuff
	{ \myVocalsStuff \transpose c c' \voPart }
	\voLyrics
    >>
    \tag #'(allPart susanPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Steelpan" }
	shortInstrumentName = \markup { \rotate #90 "Sp" }
    } <<
	\markupStuff
	{ \myVocalsStuff \transpose c c' \pianoOnePart }
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarPart
    >>
    \tag #'(allPart bassPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Bass" }
	shortInstrumentName = \markup { \rotate #90 "Ba" }
    } <<
	\markupStuff
	\clef "bass_8"
	\bassPart
    >>
    \tag #'(bassPart) \new TabStaff \with {
	stringTunings = \stringTuning <e,,, a,,, d,, g,, c, f,>
	instrumentName = \markup { \rotate #90 "eadgcf" }
    } <<
	\markupStuff
	\bassPart
    >>
    \tag #'(allPart kavPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Drums" }
	shortInstrumentName = \markup { \rotate #90 "Dr" }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
    >>
    \tag #'(ggPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
	\voLyrics
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drumsPart
    \tag #'(midiSusan) \new Staff \with {
	midiInstrument = #"steel drums"
    } \transpose c c' \pianoOnePart
    \tag #'(midiOne) \new Staff \with {
	midiInstrument = #"acoustic grand" % 0
    } \pianoTwoPart
    \tag #'(midiOne) \new Staff \with {
	midiInstrument = #"acoustic grand" % 0
    } \pianoThreePart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"electric guitar (clean)" % 27
    } \guitarPart
    \tag #'(midiBass) \new Staff \with {
	midiInstrument = #"electric bass (finger)" % 33
    } \bassPart
    \tag #'(midiTwo) \new Staff \with {
	midiInstrument = #"string ensemble 1" % 48
	midiMinimumVolume = #0
	midiMaximumVolume = #.3
    } \stringsOnePart
    \tag #'(midiTwo) \new Staff \with {
	midiInstrument = #"string ensemble 1" % 48
	midiMinimumVolume = #0
	midiMaximumVolume = #.3
    } \stringsTwoPart
    \tag #'(midiKaraoke) \new Staff <<
	\karaokeLyrics
	\transpose c c' \voPart
    >>
>>
