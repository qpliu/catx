#!/bin/sh
WHERE="https://www.songsterr.com/a/wsa/lig-lig-drum-tab-s475342"
SCALE="c des d ees e f fis g aes a bes b"

java -jar ../../songsterrToLy/1.jar --name voPart --scale "$SCALE" --no-string-numbers "${WHERE}t0" >generated-vo
java -jar ../../songsterrToLy/1.jar --name pianoOnePart --scale "$SCALE" --no-string-numbers "${WHERE}t1" >generated-piano1
java -jar ../../songsterrToLy/1.jar --name pianoTwoPart --scale "$SCALE" --no-string-numbers "${WHERE}t2" >generated-piano2
java -jar ../../songsterrToLy/1.jar --name pianoThreePart --scale "$SCALE" --no-string-numbers "${WHERE}t3" >generated-piano3
java -jar ../../songsterrToLy/1.jar --name guitarPart --scale "$SCALE" --output-tabs "${WHERE}t4" >generated-guitartabs
java -jar ../../songsterrToLy/1.jar --name bassPart --scale "$SCALE" --output-tabs "${WHERE}t5" >generated-basstabs
java -jar ../../songsterrToLy/1.jar --name stringsOnePart --scale "$SCALE" --no-string-numbers "${WHERE}t6" >generated-strings1
java -jar ../../songsterrToLy/1.jar --name stringsTwoPart --scale "$SCALE" --no-string-numbers "${WHERE}t7" >generated-strings2
#java -jar ../../songsterrToLy/1.jar --name drumsPart "${WHERE}t8" >generated-drumscolors
