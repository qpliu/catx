% BIN midi all kav susan gg peter bass
% AARIN

\header {
    title = "Smells Like Teen Spirit"
    composer = "Nirvana"
}

\include "markup"
\include "generated-bass"
\include "drums"
\include "generated-guitar1"
\include "generated-guitar2"
\include "generated-guitar3"
\include "generated-guitar4"
\include "lyrics"
\include "generated-vo"
\include "karaoke"

layoutmusic = <<
    \tag #'(allPart kavPart susanPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Vocals" }
	shortInstrumentName = \markup { \rotate #90 "Vo" }
    } <<
	\markupStuff
	\clef "treble_8"
	{ \myVocalsStuff \voPart }
	\voLyrics
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar 1" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr.1" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarOnePart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarOnePart
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar 2" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr.2" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarTwoPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarTwoPart
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar 3" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr.3" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarThreePart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarThreePart
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar 4" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr.4" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarFourPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarFourPart
    >>
    \tag #'(allPart bassPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Bass" }
	shortInstrumentName = \markup { \rotate #90 "Ba" }
    } <<
	\markupStuff
	\clef "bass_8"
	\bassPart
    >>
    \tag #'(bassPart) \new TabStaff \with { stringTunings = #bass-tuning } <<
	\markupStuff
	\bassPart
    >>
    \tag #'(allPart kavPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Drums" }
	shortInstrumentName = \markup { \rotate #90 "Dr" }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
    >>
    \tag #'(ggPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
	\voLyrics
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drumsPart
    \tag #'(midiSusan) \new Staff \with {
	midiInstrument = #"steel drums"
    } \voPart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"electric guitar (clean)" % 27
    } \guitarOnePart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"distorted guitar" % 30
	midiMinimumVolume = #0
	midiMaximumVolume = #.2
    } \guitarTwoPart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"distorted guitar" % 30
	midiMinimumVolume = #0
	midiMaximumVolume = #.2
    } \guitarThreePart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"overdriven guitar" % 29
	midiMinimumVolume = #0
	midiMaximumVolume = #.2
    } \guitarFourPart
    \tag #'(midiBass) \new Staff \with {
	midiInstrument = #"slap bass 1" % 36
    } \bassPart
    \tag #'(midiKaraoke) \new Staff \with {
	midiInstrument = #"flute" % 73
    } <<
	\karaokeLyrics
	\voPart
    >>
>>
