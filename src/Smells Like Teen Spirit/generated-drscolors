drs = \new DrumVoice = "drsb" \new DrumVoice = "drsa" \drummode {
    \context DrumVoice = "drsa" \voiceOne
    \context DrumVoice = "drsb" \voiceTwo
\mymark "Intro" #1
    r1 |
    r1 |
    r1 |
    << { <sn sn>8 hho <sn sn> hho <sn sn> hho <sn sn>4 | } \context DrumVoice = "drsb" { bd4 bd bd bd8 bd | } >>
    \repeat volta 4 {
        << { cymcb4 <hho sn>8. sn16 hho sn8. <hho sn>4 | } \context DrumVoice = "drsb" { bd2 bd4. bd8 | } >>
    } \alternative { {
        << { cymcb4 <hho sn>8. sn16 hho sn8. <hho sn>4 | } \context DrumVoice = "drsb" { bd2 bd4. bd8 | } >>
    } {
        << { cymcb4 <hho sn> hho8 sn16 sn sn sn sn sn | } \context DrumVoice = "drsb" { bd8 bd4 bd8 bd2 | } >>
    } }
\mymark "Verse 1" #8
