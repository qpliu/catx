drumstuff = <<
    \tag #'midiOne \new Staff = "notes_dotext" \notesPartDotext
    \tag #'midiOne \new Staff = "notes" \with {
	midiInstrument = #"drums"
%	midiInstrument = #"room drums"
%	midiInstrument = #"power drums"
%	midiInstrument = #"electronic drums"
%	midiInstrument = #"tr-808 drums"
%	midiInstrument = #"jazz drums"
%	midiInstrument = #"brush drums"
%	midiInstrument = #"orchestra drums"
%	midiInstrument = #"sfx drums"
%	midiInstrument = #"mt-32 drums"
	midiMinimumVolume = #0
	midiMaximumVolume = #1
    } {
	\notesPart
    }
>>

yestext = <<
    \tag #'midiOne \new Staff = "notes_dotext" \notesPartDotext
    \tag #'midiOne \new Staff = "notes" \notesPart
>>

%midimusic = \notext
%midimusic = \yestext
%midimusic = \drumstuff
midimusic = \new DrumStaff \drummode {
    \tempo 4 = 110
    \repeat unfold 10 {
	bd4 sn4 bd4 sn4 |
	bd4 sn4 bd8 sn8 sn sn16 sn |
    }
}
