thirtysix = c, % Bass Drum 1
thirtyseven = cis, % Side Stick
thirtyeight = d, % Acoustic Snare
thirtynine = dis, % Hand Clap
forty = e, % Electric Snare
fortyone = f, %  Low Floor Tom
fortytwo = fis, % Closed Hi-Hat
fortythree = g, % High Floor Tom
fortyfour = gis, % Pedal Hi-Hat
fortyfive = a, % Low Tom
fortysix = ais, % Open Hi-Hat
fortyseven = b, % Low-Mid Tom
fortyeight = c % Hi-Mid Tom
