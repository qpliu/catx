fiftyfive = g % Splash Cymbal
fiftysix = gis % Cowbell
fiftyseven = a % Crash Cymbal 2
fiftyeight = ais % Vibraslap
fiftynine = b % Ride Cymbal 2
sixty = c' % Hi Bongo
sixtyone = cis' % Low Bongo
sixtytwo = d' % Mute Hi Conga
sixtythree = dis' % Open Hi Conga
sixtyfour = e' % Low Conga
sixtyfive = f' % High Timbale
sixtysix = fis' % Low Timbale
sixtyseven = g' % High Agogo
sixtyeight = gis' % Low Agogo
sixtynine = a' % Cabasa
seventy = ais' % Maracas
seventyone = b' % Short Whistle
seventytwo = c'' % Long Whistle
seventythree = cis'' % Short Guiro
seventyfour = d'' % Long Guiro
seventyfive = dis'' % Claves
seventysix = e'' % Hi Wood Block
