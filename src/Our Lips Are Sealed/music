% BIN midi all kav susan gg peter

\header {
    title = "Our Lips Are Sealed"
    composer = "Go-Go's"
}

\include "markup"
\include "drums"
\include "generated-bass"
\include "generated-guitar"
\include "generated-guitar2"
\include "vo"
\include "lyrics"

layoutmusic = <<
    \tag #'(allPart kavPart susanPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Vocals" }
	shortInstrumentName = \markup { \rotate #90 "Vo" }
    } <<
	\markupStuff
	\clef "treble_8"
	\voPart
	\voLyrics
    >>
%    \tag #'(allPart) \new Staff \with {
%	instrumentName = \markup { \rotate #90 "Vocals2" }
%	shortInstrumentName = \markup { \rotate #90 "Vo2" }
%    } <<
%	\markupStuff
%	\clef "treble_8"
%	\voTwoPart
%    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarPart
    >>
    \tag #'(allPart peterPart) <<
	\new Staff \with {
	    instrumentName = \markup { \rotate #90 "Guitar2" }
	    shortInstrumentName = \markup { \rotate #90 "Gtr2" }
	} <<
	    \markupStuff
	    \clef "treble_8"
	    \guitarTwoPart
	>>
    >>
    \tag #'(peterPart) \new TabStaff <<
	\markupStuff
	\guitarTwoPart
    >>
    \tag #'(allPart peterPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Bass" }
	shortInstrumentName = \markup { \rotate #90 "Ba" }
    } <<
	\markupStuff
	\clef "bass_8"
	\bassPart
    >>
    \tag #'(peterPart) \new TabStaff \with { stringTunings = #bass-tuning } <<
	\markupStuff
	\bassPart
    >>
    \tag #'(allPart kavPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Drums" }
	shortInstrumentName = \markup { \rotate #90 "Dr" }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
    >>
    \tag #'(ggPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drumsPart
	\voPart
	\voLyrics
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drumsPart
    \tag #'(midiSusan) \new Staff \with {
	midiInstrument = #"steel drums"
    } \voPart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"electric guitar (clean)" % 27
    } \guitarPart
    \tag #'(midiPeter) \new Staff \with {
	midiInstrument = #"distorted guitar" % 30
	midiMinimumVolume = #0
	midiMaximumVolume = #.3
    } \guitarTwoPart
    \tag #'(midiOne) \new Staff \with {
	midiInstrument = #"electric bass (pick)"
    } \bassPart
%    \tag #'(midiKaraoke) <<
%	\karaokeLyrics
%	\snakesPart
%    >>
>>
