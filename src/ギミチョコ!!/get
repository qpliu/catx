#!/bin/bash
set -e -o pipefail
WHERE="/share/mirror/songsterr/stuff/d-keep/d-Babymetal/d-Gimme Choco-s392595/d-r397111/application_guitar_pro5-41439375.gp5"
SCALE=""
rm -f generated-*
java -jar ../../guitarproToLy/1.jar <"$WHERE" \
markup-extra '\key b \major' \
scale "$SCALE" \
track 7 \
    preset kav \
    output-lilypond \
    output-lyrics \
    output-karaoke \
track 7 \
    preset susan \
track 0 \
    preset peter \
    output-dotext \
track 4 \
    preset bass \
track 6 \
    preset midiOne \
    output-dotext \
track 3 \
    preset midiTwo \
    output-dotext \
track 2 \
    preset midiThree \
    output-dotext \
track 1 \
    preset midiFour \
    output-dotext \
track 5 \
    preset gg \
2>&1|tee log
