#!/bin/sh
# https://www.songsterr.com/api/meta/422038/revisions
# https://d12drcwhcokzqv.cloudfront.net/45581703.gp5
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key g \major' \
track 3 preset kav output-lilypond output-lyrics which-lyrics 0 layout-extra '\clef "treble_8"' \
track 0 preset peter output-lilypond \
track 1 preset midiOne output-lilypond \
track 2 preset midiTwo output-lilypond \
track 3 preset susan output-lilypond \
track 4 preset bass output-lilypond
