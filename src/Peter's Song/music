% PARTS midi
% BIN midi all

\header {
    title = "Peter's Song"
}

\include "markup"
\include "drs"

layoutmusic = <<
    \tag #'(allPart ggPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drsPart
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
>>
