#!/bin/sh
set -e
java -jar ../../songsterrToLy/1.jar --name bassPart - <bass.html >generated-bass
java -jar ../../songsterrToLy/1.jar --name bassShiftPart --shift 23/5 - <bass.html >generated-bass-shift
java -jar ../../songsterrToLy/1.jar --name drsPart - <drs.html >generated-drs
java -jar ../../songsterrToLy/1.jar --name voLyrics --lyrics - <lyrics.html >generated-lyrics
java -jar ../../songsterrToLy/1.jar --name voPart --output-relative --no-string-numbers - <lyrics.html >generated-vo
java -jar ../../songsterrToLy/1.jar --name deadnotePart --output-tabs - <deadnote.html >generated-deadnotetabs
java -jar ../../songsterrToLy/1.jar --name slidePart --output-tabs - <slide.html >generated-slidetabs
java -jar ../../songsterrToLy/1.jar --name slideText --output-text - <slide.html >generated-slidetext
java -jar ../../songsterrToLy/1.jar --name bendPart --output-tabs - <bend.html >generated-bendtabs
java -jar ../../songsterrToLy/1.jar --name bendText --output-text - <bend.html >generated-bendtext
