#!/bin/sh
# https://www.songsterr.com/api/meta/8445/revisions
java -jar ../../guitarproToLy/1.jar < song.gp5 \
markup-extra 0 '\key a \major' \
track 2 preset kav output-lilypond output-lyrics which-lyrics text layout-extra '\clef "treble_8"' \
track 0 preset peter output-lilypond \
track 1 preset gg output-drums \
track 3 preset bass output-lilypond \
track 2 preset susan output-lilypond output-lyrics which-lyrics text layout-extra '\clef "treble_8"'

