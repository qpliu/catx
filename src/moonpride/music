\include "key1text_midi"
\include "eg1"
\include "eg1text_lyrics"
\include "eg1text_midi"
\addQuote "eg1" \egOnePart
\addQuote "lyricseg1" \lyricsegOneText
\addQuote "midieg1" \midiegOneText
\include "eg2"
\include "eg2text_lyrics"
\include "eg2text_midi"
\include "eb"
\include "ebtext_lyrics"
\include "ebtext_midi"
