% vo1 vo2 piano key1 eg1 eg2 eb perc drs eg3 eg4 eg5 strings key2 key3
% system1 system2 system3
% midi
% PARTS midi
% MIDIS midiOne midiTwo midiThree
% ROLAND MOON PRIDE

\header {
    title = "MOON PRIDE"
    composer = "Revo"
}

myLayout = \layout {
    indent = 1\cm
    short-indent = 1\cm
    \context { \Score \omit BarNumber }
    \context { \Score \override SpacingSpanner.base-shortest-duration = #(ly:make-moment 1/16) }
}

drumPitchNames.barchimes = #'barchimes
midiDrumPitches.barchimes = c'''
drumPitchNames.noise = #'noise
midiDrumPitches.noise = d,, % 69
drumPitchNames.eperc = #'ridebell

#(define myDrumStyleTable '(
    (barchimes      xcircle  #f          6)
    (crashcymbal    cross    #f          6)
    (ridecymbal     cross    #f          5)
    (hihat          cross    #f          4)
    (openhihat      cross    "open"      4)
    (closedhihat    cross    "stopped"   4)
    (halfopenhihat  cross    "halfopen"  4)
    (hightom        default  #f          2)
    (ridebell       cross    #f          1)
    (snare          default  #f          1)
    (himidtom       default  #f          0)
    (lowtom         default  #f          -1)
    (noise          cross    #f          -1)
    (bassdrum       default  #f          -3)
))

