layoutmusic = \removeWithTag #'(pageNumber textLyrics) <<
    \tag #'systemOnePart \new ChordNames \mychords
    \tag #'(systemOnePart voOnePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Vo.1" }
	shortInstrumentName = \markup { \rotate #90 "Vo.1" }
    } <<
	\markupStuff
	\voOnePart
	\voOneLyrics
    >>
    \tag #'(systemOnePart choOnePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Cho.1" }
	shortInstrumentName = \markup { \rotate #90 "Cho.1" }
    } <<
	\markupStuff
	\choOnePart
    >>
    \tag #'(systemOnePart voTwoPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Vo.2" }
	shortInstrumentName = \markup { \rotate #90 "Vo.2" }
    } <<
	\markupStuff
	\voTwoPart
	\voTwoLyrics
    >>
    \tag #'(systemOnePart pianoPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Piano" }
	shortInstrumentName = \markup { \rotate #90 "Piano" }
    } <<
	\markupStuff
	\pianoPartStaffOne
	\tag #'(textLyrics keep) \lyricspianoTextStaffOne
    >>
    \tag #'(systemOnePart pianoPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Piano" }
	shortInstrumentName = \markup { \rotate #90 "Piano" }
    } <<
	\markupStuff
	\pianoPartStaffTwo
    >>
    \tag #'(systemOnePart keyOnePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Key.1" }
	shortInstrumentName = \markup { \rotate #90 "Key.1" }
    } <<
	\markupStuff
	\keyOnePart
	\tag #'(textLyrics keep) \lyricskeyOneText
    >>
    \tag #'(systemOnePart egOnePart) \new Staff \with {
	\consists "Balloon_engraver"
	instrumentName = \markup { \rotate #90 "E.G.1" }
	shortInstrumentName = \markup { \rotate #90 "E.G.1" }
    } <<
	\markupStuff
	\egOnePart
	\tag #'(textLyrics keep) \lyricsegOneText
    >>
    \tag #'(systemOnePart egTwoPart) \new Staff \with {
	\consists "Balloon_engraver"
	instrumentName = \markup { \rotate #90 "E.G.2" }
	shortInstrumentName = \markup { \rotate #90 "E.G.2" }
    } <<
	\markupStuff
	\egTwoPart
	\tag #'(textLyrics keep) \lyricsegTwoText
    >>
    \tag #'(systemOnePart ebPart) \new Staff \with {
	\consists "Balloon_engraver"
	instrumentName = \markup { \rotate #90 "E.B." }
	shortInstrumentName = \markup { \rotate #90 "E.B." }
    } <<
	\markupStuff
	\ebPart
	\tag #'(textLyrics keep) \lyricsebText
    >>
    \tag #'(systemOnePart percPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Perc." }
	shortInstrumentName = \markup { \rotate #90 "Perc." }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\percPart
	\tag #'(textLyrics keep) \lyricspercText
    >>
    \tag #'(systemOnePart drsPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Drs." }
	shortInstrumentName = \markup { \rotate #90 "Drs." }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drsPart
	\tag #'(textLyrics keep) \lyricsdrsText
    >>
    \tag #'(systemTwoPart egThreePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "E.G.3" }
	shortInstrumentName = \markup { \rotate #90 "E.G.3" }
    } <<
	\markupTwoStuff
	\egThreePart
	\tag #'(textLyrics keep) \lyricsegThreeText
    >>
    \tag #'(systemTwoPart egFourPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "E.G.4" }
	shortInstrumentName = \markup { \rotate #90 "E.G.4" }
    } <<
	\markupTwoStuff
	\egFourPart
	\tag #'(textLyrics keep) \lyricsegFourText
    >>
    \tag #'(systemTwoPart egFivePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "E.G.5" }
	shortInstrumentName = \markup { \rotate #90 "E.G.5" }
    } <<
	\markupTwoStuff
	\egFivePart
	\tag #'(textLyrics keep) \lyricsegFiveText
    >>
    \tag #'(systemThreePart stringsPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Strings" }
	shortInstrumentName = \markup { \rotate #90 "Strings" }
    } <<
	\markupThreeStuff
	\stringsPartStaffOne
    >>
    \tag #'(systemThreePart stringsPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Strings" }
	shortInstrumentName = \markup { \rotate #90 "Strings" }
    } <<
	\markupThreeStuff
	\stringsPartStaffTwo
    >>
    \tag #'(systemThreePart stringsPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Strings" }
	shortInstrumentName = \markup { \rotate #90 "Strings" }
    } <<
	\markupThreeStuff
	\stringsPartStaffThree
    >>
    \tag #'(systemThreePart keyTwoPart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Key.2" }
	shortInstrumentName = \markup { \rotate #90 "Key.2" }
    } <<
	\markupThreeStuff
	\keyTwoPart
	\tag #'(textLyrics keep) \lyricskeyTwoText
    >>
    \tag #'(systemThreePart keyThreePart) \new Staff \with {
	instrumentName = \markup { \rotate #90 "Key.3" }
	shortInstrumentName = \markup { \rotate #90 "Key.3" }
    } <<
	\markupThreeStuff
	\keyThreePart
	\tag #'(textLyrics keep) \lyricskeyThreeText
    >>
>>

midimusicAll = <<
    \markupStuff
    \tag #'(midiOne voPart) \new Staff \with {
	midiInstrument = #"synth voice"
	midiMinimumVolume = #0
	midiMaximumVolume = #1
    } \voOnePart
    \tag #'(midiOne voPart) \new Staff \with {
	midiInstrument = #"synth voice"
	midiMinimumVolume = #0
	midiMaximumVolume = #1
    } \voTwoPart
    \tag #'(midiOne choPart) \new Staff \with {
	midiInstrument = #"choir aahs"
	midiMinimumVolume = #0
	midiMaximumVolume = #1
    } \choOnePart
    \tag #'(midiOne pianoPartStaffOne) \new Staff = "pianoStaff1text" \midipianoTextStaffOne
    \tag #'(midiOne pianoPartStaffOne) \new Staff = "pianoStaff1" \pianoPartStaffOne
    \tag #'(midiOne pianoPartStaffTwo) \new Staff \with {
	midiInstrument = #"acoustic grand"
	midiMinimumVolume = #0
	midiMaximumVolume = #1
    } \pianoPartStaffTwo
    \tag #'(midiOne keyOnePart) \new Staff = "key1text" \midikeyOneText
    \tag #'(midiOne keyOnePart) \new Staff = "key1" \keyOnePart
    \tag #'(midiOne keyTwoPart) \new Staff = "key2text" \midikeyTwoText
    \tag #'(midiOne keyTwoPart) \new Staff = "key2" \keyTwoPart
    \tag #'(midiTwo keyThreePart) \new Staff = "key3text" \midikeyThreeText
    \tag #'(midiTwo keyThreePart) \new Staff = "key3" \keyThreePart
    \tag #'(midiTwo egOnePart) \new Staff = "eg1text" \midiegOneText
    \tag #'(midiTwo egOnePart) \new Staff = "eg1" \egOnePart
    \tag #'(midiTwo egTwoPart) \new Staff = "eg2text" \midiegTwoText
    \tag #'(midiTwo egTwoPart) \new Staff = "eg2" \egTwoPart
    \tag #'(midiTwo egThreePart) \new Staff = "eg3text" \midiegThreeText
    \tag #'(midiTwo egThreePart) \new Staff = "eg3" \egThreePart
    \tag #'(midiTwo egFourPart) \new Staff = "eg4text" \midiegFourText
    \tag #'(midiTwo egFourPart) \new Staff = "eg4" \egFourPart
    \tag #'(midiThree egFivePart) \new Staff = "eg5text"  \midiegFiveText
    \tag #'(midiThree egFivePart) \new Staff = "eg5" \egFivePart
    \tag #'(midiThree ebPart) \new Staff = "ebtext" \midiebText
    \tag #'(midiThree ebPart) \new Staff = "eb" \ebPart
    \tag #'(midiThree percPart) \new Staff = "perctext" \midipercText
    \tag #'(midiThree percPart) \new DrumStaff = "perc" \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \percPart
    \tag #'(midiThree drsPart) \new Staff = "drstext"  \mididrsText
    \tag #'(midiThree drsPart) \new DrumStaff = "drs" \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
    \tag #'(midiThree violinPart) \new Staff \with {
	midiInstrument = #"violin"
	midiMinimumVolume = #0
	midiMaximumVolume = #.7
    } \stringsPartStaffOne
    \tag #'(midiThree celloPart) \new Staff \with {
	midiInstrument = #"cello"
    } \stringsPartStaffTwo
    \tag #'(midiThree contrabassPart) \new Staff \with {
	midiInstrument = #"contrabass"
    } \stringsPartStaffThree
>>

midimusic = <<
%    \keepWithTag #'(voPart keep) \midimusicAll
%    \keepWithTag #'(choPart keep) \midimusicAll
%    \keepWithTag #'(pianoPartStaffOne keep) \midimusicAll
%    \keepWithTag #'(pianoPartStaffTwo keep) \midimusicAll
%    \keepWithTag #'(keyOnePart keep) \midimusicAll
%    \keepWithTag #'(keyTwoPart keep) \midimusicAll
%    \keepWithTag #'(keyThreePart keep) \midimusicAll
%    \keepWithTag #'(egOnePart keep) \midimusicAll
%    \keepWithTag #'(egTwoPart keep) \midimusicAll
%    \keepWithTag #'(egThreePart keep) \midimusicAll
%    \keepWithTag #'(egFourPart keep) \midimusicAll
%    \keepWithTag #'(egFivePart keep) \midimusicAll
%    \keepWithTag #'(ebPart keep) \midimusicAll
%    \keepWithTag #'(percPart keep) \midimusicAll
%    \keepWithTag #'(drsPart keep) \midimusicAll
%    \keepWithTag #'(violinPart keep) \midimusicAll
%    \keepWithTag #'(celloPart keep) \midimusicAll
%    \keepWithTag #'(contrabassPart keep) \midimusicAll
    \midimusicAll
>>

rolandmusic = <<
    \markupStuff
    \tag #'(rolandMelody) \new Staff \with {
	midiInstrument = #"synth voice"
    } <<
	\voOnePart
	\voTwoPart
	\choOnePart
    >>
    \tag #'(rolandBass) \new Staff \with {
	midiInstrument = #"electric bass (pick)" % 34
    } <<
	\ebPart
    >>
    \tag #'(rolandBackOne) \new Staff \with {
	midiInstrument = #"electric guitar (clean)" % 27
    } <<
	\egOnePart
	\egTwoPart
	\egThreePart
	\egFourPart
	\egFivePart
	\stringsPartStaffOne
	\stringsPartStaffTwo
	\stringsPartStaffThree
    >>
    \tag #'(rolandBackTwo) \new Staff \with {
	midiInstrument = #"acoustic grand"
    } <<
	\pianoPartStaffOne
	\pianoPartStaffTwo
	\keyOnePart
	\keyTwoPart
	\keyThreePart
    >>
    \tag #'(rolandDrums) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
    \tag #'(rolandPerc) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
	midiMinimumVolume = #10
	midiMaximumVolume = #10
    } <<
	\percPart
    >>
>>
