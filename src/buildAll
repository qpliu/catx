#!/bin/sh
set -e
pwd=`pwd`
if test '!' -f "build1"
then
    echo "Wrong directory"
    exit 1
fi
for a in */music
do
    name=`dirname "$a"`
    if test "x$name" '!=' "xbin"
    then
	PARTS=`grep '^% BIN ' "$a"|cut -c 7-`
	if test -n "$PARTS"
	then
	    cd "$name"
	    echo "$name"
	    ../build1 $PARTS
	    cd ..
	fi
    fi
done
