    }
% 1.3
    \mybarNumberCheck #9
    << { <sn toml>4 <sn toml>8 <sn toml> <sn toml> <sn toml> <sn toml> <sn toml> | } \context DrumVoice = "drsb" { bd4 bd8 bd bd bd bd bd | } >>
    << { <sn toml>8 <sn toml> <sn toml> <sn toml> <sn toml>4 <sn sn> | } \context DrumVoice = "drsb" { bd8 bd bd bd bd bd r4 | } >>
