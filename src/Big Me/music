% system1 drs midi roland
% PARTS drs

\header {
    title = "Big Me"
    composer = "Foo Fighters"
}

\paper {
    system-system-spacing = #'((basic-distance . 18) (minimum-distance . 12) (padding . 1) (stretchability . 90))
}

#(set-global-staff-size 20)
#(set-default-paper-size "letter" 'portrait)

\include "markup"
\include "drs"

layoutmusic = <<
    \tag #'(systemOnePart drsPart) \new DrumStaff \with {
	instrumentName = \markup { \rotate #90 "Drums" }
	shortInstrumentName = \markup { \rotate #90 "Dr" }
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drsPart
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiGg rolandDrums) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
>>
