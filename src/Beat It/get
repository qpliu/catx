#!/bin/sh
WHERE="application_guitar_pro5-41656752.gp5"
set -e
rm -f generated-*
java -jar ../../guitarproToLy/1.jar <"$WHERE" \
markup-extra 0 '\key dis \minor' \
track 0 \
    preset kav \
    output-lilypond \
    output-lyrics \
    output-karaoke \
    which-lyrics "0" \
track 0 \
    preset susan \
    output-lilypond \
track 1 \
    preset peter \
    output-dotext \
    output-lilypond \
track 2 \
    preset peter \
    output-lilypond \
track 3 \
    preset peter \
    output-lilypond \
track 4 \
    preset bass \
    output-lilypond \
track 5 \
    preset midiOne \
    output-lilypond \
track 7 \
    preset midiOne \
    output-lilypond \
track 6 \
    preset gg \
    output-drums \
2>&1|tee log
