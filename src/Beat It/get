#!/bin/sh
WHERE="/share/mirror/songsterr/stuff/d-keep/d-Michael Jackson/d-Beat It-s10259/d-r399557/application_guitar_pro5-41656752.gp5"
set -e
rm -f generated-*
java -jar ../../guitarproToLy/1.jar <"$WHERE" \
modified-filename vocals_karaoke \
track 0 \
    preset kav \
    output-lilypond \
    output-lyrics \
    output-karaoke \
track 0 \
    preset susan \
track 1 \
    preset peter \
    output-dotext \
track 2 \
    preset peter \
    name guitarTwo \
    instrument-name Guitar2 \
    instrument-short-name Gtr2 \
track 3 \
    preset peter \
    name guitarThree \
    instrument-name Guitar3 \
    instrument-short-name Gtr3 \
track 4 \
    preset bass \
track 5 \
    preset midiOne \
track 7 \
    preset midiTwo \
track 6 \
    preset gg \
2>&1|tee log
