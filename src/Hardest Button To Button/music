% PARTS midi

\header {
    title = "Hardest Button To Button"
    composer = "The White Stripes"
}

#(set-global-staff-size 20)

\include "markup"
\include "drs"
\include "vo"
\include "lyrics"
\include "chorus"
\include "choruslyrics"

layoutmusic = <<
    \tag #'(allPart) \new Staff \with { \RemoveEmptyStaves } <<
	\markupStuff
	\voPart
	\voLyrics
    >>
    \tag #'(allPart) \new Staff \with { \RemoveEmptyStaves } <<
	\markupStuff
	\chorusPart
	\chorusLyrics
    >>
    \tag #'(allPart) \new DrumStaff \with {
	drumStyleTable = #(alist->hash-table myDrumStyleTable)
    } <<
	\markupStuff
	\drsPart
    >>
>>

midimusic = <<
    \markupStuff
    \tag #'(midiSusan) \new Staff \with {
	midiInstrument = #"steel drums" % 114
    } <<
	\voPart
	\chorusPart
    >>
    \tag #'(midiGg) \new DrumStaff \with {
	drumPitchTable = #(alist->hash-table midiDrumPitches)
    } \drsPart
>>
