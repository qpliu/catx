#!/bin/sh
set -e
if test '!' -f "buildAll"
then
    echo "Wrong directory.  Run this from the build directory."
    exit 1
fi
rm -rf /share/music/mp3/*.mp3 /share/music/pdf/*.pdf /share/music/midi/*.midi /share/music/aarin/*.aarin
cd ../src
for a in */music
do
    name=`dirname "$a"`
    cd "$name"
    ../buildBin
    cd ..
done
